{
  "presets": [
    ["@babel/env", {
      "targets": {
        "browsers": ["last 2 versions", "chrome >= 41"]
      },
      "modules": false
    }],
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/plugin-transform-object-rest-spread",
    "@babel/plugin-transform-class-properties"
  ]
}
